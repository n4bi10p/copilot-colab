# Copy this file to .env and fill in your Supabase credentials
# Get these from: https://supabase.com/dashboard/project/<your-project>/settings/api
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.5-flash

GITHUB_TOKEN=your_github_token
# optional fallback; workspace git origin is auto-detected first
GITHUB_REPOSITORY=n4bi10p/copilot-colab

# optional Copilot SDK defaults
COPILOT_MODEL=gpt-4.1
# optional if using external CLI server (`copilot --headless --port 4321`)
# must be a URL reachable from the extension host runtime
COPILOT_CLI_URL=http://localhost:4321
